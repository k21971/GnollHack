<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:skia="clr-namespace:SkiaSharp.Views.Maui.Controls;assembly=SkiaSharp.Views.Maui.Controls"
             xmlns:gnh="clr-namespace:GnollHackM" 
             xmlns:controls="clr-namespace:GnollHackM" 
             x:Name="this"
             x:DataType="controls:CustomImageButton"
             x:Class="GnollHackM.CustomImageButton">
  
        <Grid x:Name="customGrid" CascadeInputTransparent="True" HorizontalOptions="Fill" VerticalOptions="Fill">
            <gnh:CustomCanvasView x:Name="customCanvasView" HorizontalOptions="Fill" VerticalOptions="Fill" EnableTouchEvents="false" PaintSurface="CustomCanvasView_PaintSurface" />
            <gnh:NoTabStopButton Text="" x:Name="customButton" Clicked="CustomButton_Clicked" BackgroundColor="Transparent" HorizontalOptions="Fill" VerticalOptions="Fill" />
        </Grid>
    
</ContentView>
