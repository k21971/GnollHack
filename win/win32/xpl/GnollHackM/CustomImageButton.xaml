<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:skia="clr-namespace:SkiaSharp.Views.Maui.Controls;assembly=SkiaSharp.Views.Maui.Controls"
             x:Class="GnollHackM.CustomImageButton">
  
        <Grid x:Name="customGrid" HorizontalOptions="Fill" VerticalOptions="Fill">
            <skia:SKCanvasView x:Name="customCanvasView" HorizontalOptions="Fill" VerticalOptions="Fill" EnableTouchEvents="false" PaintSurface="CustomCanvasView_PaintSurface" />
            <Button Text="" x:Name="customButton" Clicked="CustomButton_Clicked" BackgroundColor="Transparent" HorizontalOptions="Fill" VerticalOptions="Fill" />
        </Grid>
    
</ContentView>
