<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:skia="clr-namespace:SkiaSharp.Views.Forms;assembly=SkiaSharp.Views.Forms"
             xmlns:gnh="clr-namespace:GnollHackX" 
             xmlns:controls="clr-namespace:GnollHackX.Controls" 
             x:Name="this"
             x:DataType="controls:CustomImageButton"
             x:Class="GnollHackX.Controls.CustomImageButton">
  <ContentView.Content>
        <Grid x:Name="customGrid" CascadeInputTransparent="True" HorizontalOptions="Fill" VerticalOptions="Fill">
            <gnh:CustomCanvasView x:Name="customCanvasView" HorizontalOptions="Fill" VerticalOptions="Fill" EnableTouchEvents="false" PaintSurface="CustomCanvasView_PaintSurface" />
            <gnh:NoTabStopButton Text="" x:Name="customButton" Clicked="CustomButton_Clicked" BackgroundColor="Transparent" HorizontalOptions="Fill" VerticalOptions="Fill" Padding="0" />
        </Grid>
    </ContentView.Content>
</ContentView>